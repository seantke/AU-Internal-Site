      <span id="back2top">&uarr; Back to Top</span>
      {% for stage in od %}
      <div class="od-stage-container" id="#val">
        <h2>{{stage.name}}</h2>
        <div>Primary Responsibility:{{stage.primary_responsibility}}
        </div>
        <div class="od-stage-datasets">
          <h3>Datasets currently in this Stage:</h3>
          {% for dataset in stage.datasets %}
          {% if dataset != "" %}
          <div><a href="{{site.url}}/?p={{dataset}}"><i class="fa fa-database"></i> {{ function('get_post_field', 'post_title', dataset) }}</a></div>
          {% else %}
          <div><i class="fa fa-database"></i> No dataset in this stage</div>
          {% endif %}
          {% endfor %}
        </div>
        <div class="od-stage-card">
          <div class="od-stage-title togglebox">
            <h3>
              <i class="fa fa-plus"></i>
              Objectives:</h3>
          </div>
          <div class="od-stage-content togglecontent">
            {{stage.objectives}}
          </div>
        </div>
        <div class="od-stage-card">
          <div class="od-stage-title togglebox">
            <h3>
              <i class="fa fa-plus"></i>
              Consulted Staff and purspose for consultation:</h3>
          </div>
          <div class="od-stage-content togglecontent">
            {{stage.consulted_staff}}
          </div>
        </div>
      </div>
      {% endfor %}
